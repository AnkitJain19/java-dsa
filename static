public class HelloWorld {
    static int a=0;
    static void method(){
        a++;
       int b=0;
        //static int c=10;  illegal start of expression
        b++;
    }
   static{
        System.out.println(a);
    }
    public static void main(String[] args) {
       HelloWorld h1= new HelloWorld();
       method();
       method();
       method();
     System.out.println(a);
       System.out.println(((Object)a).getClass().getSimpleName());
       System.out.println(a);
    }
}
